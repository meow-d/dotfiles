{
    "autosu": true,
    "colorscheme": "monokai-dark-transparent",
    "divchars": "│─",
    "filemanager-openonstart": true,
    "filemanager-showignored": false,
    "hlsearch": true,
    "softwrap": true,
    "statusformatl": " $(filename) $(modified)$(status.paste)",
    "statusformatr": "($(line),$(col)) $(opt:filetype) ",
    "tabmovement": true,
    "tabstospaces": true,
    "wordwarp": true
}
