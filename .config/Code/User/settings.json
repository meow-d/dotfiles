{
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "code-runner.runInTerminal": true,
  "python.showStartPage": false,
  "python.languageServer": "Pylance",
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true
  },
  "git.enableSmartCommit": true,
  "gitlens.views.repositories.location": "gitlens",
  "gitlens.views.fileHistory.location": "gitlens",
  "gitlens.views.lineHistory.location": "gitlens",
  "gitlens.views.compare.location": "gitlens",
  "gitlens.views.search.location": "gitlens",
  "terminal.integrated.fontFamily": "SauceCodePro Nerd Font",
  "python.autoComplete.addBrackets": true,
  "tabnine.experimentalAutoImports": true,
  "glassit.alpha": 240,
  "python.analysis.completeFunctionParens": true,
  "python.formatting.provider": "black",
  "editor.formatOnSave": true,
  "terminal.integrated.rendererType": "dom",
  "vscode_vibrancy.theme": "Default Dark",
  "workbench.startupEditor": "newUntitledFile",
  "glassit-linux.opacity": 20,
  "editor.fontFamily": "Cascadia Code",
  "explorer.confirmDragAndDrop": false,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "explorer.confirmDelete": false,
  "git.confirmSync": false,
  "editor.fontLigatures": "'ss01','cv01','cv02','ss05'",
  "workbench.productIconTheme": "icons-carbon",
  "editor.bracketPairColorization.enabled": true,
  "editor.formatOnType": true,
  "dart.closingLabels": false,
  "editor.lineHeight": 24,
  "editor.fontSize": 13,
  "dart.env": {
    "CHROME_EXCUTABLE": "/usr/bin/google-chrome-stable"
  },
  "workbench.colorTheme": "Monokai Dimmed",
  "gitProjectManager.baseProjectsFolders": [
    "~/nerd-stuff"
  ],
  "window.titleBarStyle": "custom",
  "editor.minimap.renderCharacters": false,
  "editor.minimap.size": "fill",
  "editor.minimap.maxColumn": 80,
  "editor.minimap.enabled": false,
  "markdown.extension.italic.indicator": "_",
  "ltex.disabledRules": {
    "en-GB": [
      "UPPERCASE_SENTENCE_START"
    ]
  },
  "ltex.language": "en-GB",
  "terminal.external.linuxExec": "kitty",
  "terminal.integrated.fontWeight": "600",
  "editor.linkedEditing": true,
  "files.trimTrailingWhitespace": true,
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  "workbench.iconTheme": "material-icon-theme",
  "zenMode.hideLineNumbers": false,
  "editor.cursorSurroundingLines": 6,
  "markdown-links.graphType": "obsidian",
  "workbench.experimental.layoutControl.enabled": true,
  "window.menuBarVisibility": "toggle",
  "breadcrumbs.filePath": "last",
  "todo-tree.highlights.useColourScheme": true,
  "spellright.notificationClass": "warning",
  "spellright.suggestionsInHints": false,
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX"
  ],
  "markless.hoverImage": false,
  "markless.table": false,
  "markless.inlineCode": false,
  "workbench.activityBar.visible": false,
  "errorLens.gutterIconsEnabled": true,
  "errorLens.gutterIconSize": "30%",
  "errorLens.gutterIconSet": "circle",
  "markless.html": false,
  "markless.mermaid": false,
  "markless.inlineImage.autoPreview": false,
  "latex-utilities.liveReformat.enabled": true,
  "settingsSync.keybindingsPerPlatform": false,
  "gitProjectManager.maxDepthRecursion": 1,
  "files.autoSave": "afterDelay"
}